<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hrankit.roompaging">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".roomWithPaging.RoomWithPagingActivity"
            android:label="@string/title_activity_room_with_paging"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NewWordActivity" />
        <activity
            android:name=".roomWithoutPaging.RoomWithoutPagingActivity"
            android:label="@string/room_without_paging"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>